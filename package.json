  "scripts": {
    "start": "node app.js",
    "dev": "set NODE_ENV=development&& nodemon app.js",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-fix": "eslint --ignore-path .gitignore . --fix",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntonLukichev/node-imgoptimize.git"
  },
  "keywords": [
