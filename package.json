{
  "name": "node-imgoptimize",
  "version": "1.0.0",
  "description": "CDN for image optimization on NodeJS",
  "main": "app.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ignore-path .gitignore . \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntonLukichev/node-imgoptimize.git"
  },
  "keywords": [
    "CDN",
    "NodeJS",
    "image-processing",
    "Webp",
    "Jpeg optimize",
    "Typescript",
    "NestJS",
    "Fastify",
    "Axios",
    "Sharp",
    "Libvips"
  ],
  "author": {
    "name": "Anton Lukichev",
    "email": "anton@lukichev.pro"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AntonLukichev/node-imgoptimize/issues"
  },
  "homepage": "https://github.com/AntonLukichev/node-imgoptimize#readme",
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 6.x"
  },
  "dependencies": {
    "@hapi/boom": "^9.0.0",
    "@nestjs/common": "^6.11.7",
    "@nestjs/core": "^6.11.7",
    "@nestjs/platform-fastify": "^6.11.7",
    "@nestjs/swagger": "^4.2.8",
    "@sentry/node": "^5.12.3",
    "axios": "^0.19.2",
    "fastify": "^2.12.0",
    "fastify-favicon": "^2.0.0",
    "fastify-response-time": "^1.1.0",
    "fastify-static": "^2.6.0",
    "fastify-swagger": "^2.5.0",
    "fastify-url-data": "^2.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sharp": "^0.24.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.7",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-node": "^11.0.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "verbose": true
  }
}
